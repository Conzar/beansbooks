<form action="" method="POST">
	<div class="form report-form">
		<div class="row column-10">
			<span class="oop-oop inline-label text-right">Start Date</span>
			<span class="oop-oop-oop"><input type="text" class="report-sales-date" name="date_start" value="{{report_date_start}}"></span>
			<span class="oop-oop inline-label text-right">End Date</span>
			<span class="oop-oop-oop"><input type="text" class="report-sales-date" name="date_end" value="{{report_date_end}}"></span>
			<span class="oop-oop inline-label text-right">Sort By</span>
			<span class="oop-oop">
				<div class="select"><select class="report-sales-interval" name="interval">
					{{#interval_options}}
						<option value="{{value}}" {{#selected}}selected="selected"{{/selected}}>{{name}}</option>
					{{/interval_options}}
				</select></div>
			</span>
			<span class="oop-oop-oop">&nbsp;</span>
			<span class="oop-oop-oop"><a href="#" class="button report-print">Print Report</a></span>
		</div>
		<div class="clear"></div>
	</div>
</form>

<div class="clear"></div>

<div id="report-content">
	{{>dash_print_sales}}
</div>